{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Simplified Enrollment Schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": { "type": "number" },
      "child_id": { "type": "number" },
      "user_id": { "type": "number" },
      "enroll_type": { "type":["string", "null"]},
      "enroll_start_date": { "type": "string" },
      "enrollment_status": { "type": "string" },
      "status": { "type": "string" },
      "first_name": { "type": "string" },
      "last_name": { "type": "string" },
      "birthdate": { "type": "string" }
    },
    "required": [
      "id",
      "child_id",
      "user_id",
      "enroll_type",
      "enroll_start_date",
      "enrollment_status",
      "status",
      "first_name",
      "last_name",
      "birthdate"
    ]
  }
}
